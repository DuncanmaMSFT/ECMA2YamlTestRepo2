{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "add/includes/**/*.md",
          "add/metadata/**/*.md",
          "**/obj/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "/**/*.png",
          "add/**/*.jpg",
          "add/**/*.gif"
        ],
        "exclude": [
          "add/codesnippet/**/*.*",
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": {
        "files": [
          "add/metadata/**/*.md"
        ]
      },
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/fulldotnet/toc.json"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "fulldotnet"
  }
}