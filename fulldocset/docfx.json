{
  "merge": {
    "content": [
      {
        "files": [
          "*.yml"
        ],
        "exclude": [
          "add/includes/**/*.md",
          "add/metadata/**/*.md",
          "**/obj/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "dest": "_merged",
    "tocMetadata": "version"
  },
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "_merged/**/*.yml",
          "breadcrumb/toc.yml"
        ],
        "exclude": [
          "add/includes/**/*.md",
          "add/metadata/**/*.md",
          "**/obj/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "/**/*.png",
          "add/**/*.jpg",
          "add/**/*.gif"
        ],
        "exclude": [
          "add/codesnippet/**/*.*",
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "~/breadcrumb/toc.yml"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "fulldotnet"
  }
}
